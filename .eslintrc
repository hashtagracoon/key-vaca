{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "keyword-spacing": 0,
    "quotes": 0,
    "object-shorthand": 0,
    "semi-spacing": 0,
    "prefer-const": 0,
    "func-names": 0
  }
}
