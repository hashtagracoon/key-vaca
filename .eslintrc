{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "keyword-spacing": 0,
    "quotes": 0,
    "object-shorthand": 0,
    "semi-spacing": 0,
    "prefer-const": 0,
    "func-names": 0,
    "global-require": 0,
    "comma-dangle": 0,
    "space-before-function-paren": 0,
    "prefer-template": 0,
    "padded-blocks": 0,
    "brace-style": 0,
    "newline-per-chained-call": 0,
    "no-plusplus": 0,
    "spaced-comment": 0,
    "arrow-body-style": 0,
  }
}
